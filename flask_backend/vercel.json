{
    "version": 2,
    "builds": [
      {
        "src": "run.py",
        "use": "@vercel/python"
      }
    ],
    "env": {
      "SECRET_KEY": "@your-secret-key",
      "DATABASE_URI": "@your-database-uri"
    },
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "run.py"
      }
    ],
    "installCommand": "pip install -r requirements.txt",
    "buildCommand": ""
  }
  